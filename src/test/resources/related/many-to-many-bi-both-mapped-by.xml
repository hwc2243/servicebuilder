<?xml version="1.0" encoding="UTF-8"?>

<service name="test" package="m2m.bi.both.mapped.by">
	<entity name="foo">
		<key name="id" type="long"/>

		<attribute name="text" type="string"/>

        <related name="bars" entity="bar" collection="list" relationship="many_to_many" mapped-by="foos"/>
	</entity>
	
	<entity name="bar">
        <key name="id" type="long"/>

		<attribute name="text" type="string"/>
        
        <related name="foos" entity="foo" collection="list" relationship="many_to_many" mapped-by="bars"/>
    </entity>
</service>